{
  "name": "alapi/php-sdk",
  "description": "This is ALAPI PHP-SDK",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "alapi",
    "http",
    "php-sdk",
    "api",
    "alapi-sdk"
  ],
  "homepage": "https://www.alapi.cn",
  "authors": [
    {
      "name": "Alone88",
      "email": "im@alone88.cn"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "ALAPI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ALAPI\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpunit/phpunit": "^7.5"
  }
}
